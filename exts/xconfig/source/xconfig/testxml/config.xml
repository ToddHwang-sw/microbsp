		<gateway>174.100.0.1</gateway>
		<dns>8.8.8.8</dns>
		<ipv6>
			<mode>static</mode>     <!-- IPv6 DHCP -->
			<global>2001:b0d8:2233:1000::</global>
			<local>4400::</local>
			<plen>64</plen>
			<id>1</id>
		</ipv6>
		<capture>0</capture>
	</lan0>
	<lan1>
		<mode>static</mode>          <!-- static, dhcp, none -->
		<interface>eth2</interface>
		<ip>174.200.0.140</ip>
