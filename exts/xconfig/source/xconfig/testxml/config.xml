		<date>2022.01.01-12:00:00</date>  <!-- date -->
	</operation>
	<ssh>
		<enable>1</enable>          <!-- activate -->
		<port>22</port>             <!-- service port -->
		<dir>/var/tmp/myfolder</dir><!-- dir -->
