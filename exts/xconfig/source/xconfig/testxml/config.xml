	<date>202201011235</date>
	<model>TODD-GATEWAY</model>
	<operation>
		<mode>normal</mode>			<!-- operation mode -->
		<passwd>0</passwd>		<!-- password -->
		<webui>0</webui>		<!-- webui -->
		<syslog>
			<en>1</en>
			<mode>local</mode>	<!-- local -> /var/log/messages, remote -> UDP -->
			<ip>10.10.10.30</ip>
