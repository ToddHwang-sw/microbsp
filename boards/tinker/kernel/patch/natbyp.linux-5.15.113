+/* clearing one flow */
+static int natbyp_flush_flow (natbyp_flow_t * flow)
+{
+    skbq_t *q;
+    int tail;
+    int cnt = 0;
+    struct sk_buff *pskb;
+
+    if (!flow)
+        return -1;
+
+    q = (skbq_t *) & (flow->skbq);
+
+    /* clearing queue */
+    tail = q->tail;
+    while ((tail != q->head) &&
+            (cnt++ <= MAX_SKB_QUEUE)) {
+
+        if (! ((tail >= 0) && (tail < MAX_SKB_QUEUE)))
+            goto __skip_current;
+
+        pskb = q->skbs[ tail ].pkt;
+        if (pskb) {
+			// Well ~~~
+            //dev_kfree_skb (pskb);
+            q->skbs[ tail ].pkt = NULL;
+        }
+
+__skip_current:
+        ++tail;
+        if (tail == MAX_SKB_QUEUE)
+            tail = 0;
+    }
+
+    /* clear... */
+    natbyp_skbq_init (q);
+
+    return cnt;
+}
+
+/* zapping */
+static int natbyp_squeeze (void)
+{
+    int index;
+    natbyp_flow_t * flow;
+	int cnt = 0;
+
+    for (index = 0; index < MAX_NATBYP_FLOW; index++) {
+        flow = & (natbyp_flow[ index ]);
+        cnt += natbyp_flush_flow (flow);
+        flow->used = 0;
+    }
+
+	return cnt;
+}
